{
  "name": "U-SEP",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'pnpm run generate-theme --dev'",
    "dev:only-vite": "vite",
    "vite:electron": "vite --mode electron",
    "electron:start": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:5173 electron ./public/main.js",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --pretty --noEmit --project tsconfig.app.json",
    "generate-theme": "tsx scripts/generate-theme.ts",
    "prebuild": "pnpm generate-theme",
    "build:python": "cd backend && pip install -r requirements-build.txt && python auto_hidden_imports.py && pyinstaller main.spec --noconfirm",
    "build:electron": "pnpm run build:python && tsc -b && vite build --mode electron",
    "electron-start": "node scripts/setup-python.js && concurrently \"pnpm run vite:electron\" \"pnpm run electron:start\"",
    "electron-pack:win": "pnpm run build:electron && electron-builder build --publish never -c.extraMetadata.main=dist/main.js --win nsis:x64",
    "test": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-table": "^8.21.3",
    "@toss/use-overlay": "^1.4.2",
    "@toss/utils": "^1.6.1",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^9.1.2",
    "cytoscape": "^3.33.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.6.0",
    "es-hangul": "^2.3.3",
    "es-toolkit": "^1.38.0",
    "html2canvas-pro": "^1.5.11",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.475.0",
    "nanoid": "^5.1.5",
    "react": "^19.0.1",
    "react-day-picker": "9.8.1",
    "react-dom": "^19.0.1",
    "react-hook-form": "^7.54.2",
    "react-loading-indicators": "^1.0.1",
    "react-resizable-panels": "^3.0.4",
    "react-router-dom": "^7.1.5",
    "recharts": "2.15.4",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@eslint/js": "^9.19.0",
    "@hookform/devtools": "^4.4.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/node": "^22.13.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "electron": "^35.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "playwright": "^1.56.1",
    "postcss": "^8.5.2",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.12",
    "tailwindcss": "^4.0.6",
    "tsx": "^4.7.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.4.1",
    "vite-plugin-jsx": "^0.0.6",
    "vitest": "^3.1.1"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,js,jsx,css}": [
      "pnpm run format",
      "pnpm run lint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "build": {
    "appId": "com.example.gr-simulator",
    "productName": "U-SEP",
    "icon": "public/temp-logo.ico",
    "files": [
      "dist/**/*",
      "public/**/*"
    ],
    "extraFiles": [
      {
        "from": "backend/dist/main",
        "to": "resources/backend/main"
      },
      {
        "from": "backend/simulator_v060",
        "to": "resources/backend/simulator_v060"
      },
      "./springboot/**/*",
      "./python/**/*"
    ],
    "extraResources": [
      {
        "from": "public/U-SEP_Tutorial.mp4",
        "to": "videos/U-SEP_Tutorial.mp4"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "public/temp-logo.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "public/temp-logo.ico",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "U-SEP",
      "runAfterFinish": true,
      "allowElevation": true,
      "packElevateHelper": true
    }
  },
  "packageManager": "pnpm@9.15.9",
  "pnpm": {
    "overrides": {
      "brace-expansion": "1.1.12",
      "@eslint/plugin-kit": ">=0.3.4",
      "form-data": ">=4.0.4",
      "electron": ">=35.7.5",
      "tmp": ">=0.2.4"
    }
  }
}
